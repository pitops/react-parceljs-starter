{
    "parser": "babel-eslint",
    "settings": {
        "react": {
            "version": "16.6.0"
        }
    },
    "extends": [
        "standard",
        "plugin:jsx-a11y/recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "import",
        "node",
        "promise",
        "react",
        "jest",
        "jsx-a11y"
    ],
    "env": {
        "node": true,
        "jest": true
    }
}